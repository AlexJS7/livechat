{
  "extends": "airbnb",
  "rules": {
    "no-underscore-dangle": ["error", {"allow": ["_id", "_doc"]}],
    "no-console": 1,
    "react/no-array-index-key": 1,
    "global-require": 0,
    "class-methods-use-this": 0,
    "jsx-a11y/no-static-element-interactions": 0
  },
  "env": {
    "browser": true
  }
}